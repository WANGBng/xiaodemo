<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bwie">

    <uses-permission android:name="android.permission.INTERNET" /> <!-- 网络权限 -->
    <uses-permission android:name="android.permission.CAMERA" /> <!-- 调取相机权限 -->
    <uses-feature android:name="android.hardware.camera" /> <!--  使用照相机权限  -->
    <uses-feature android:name="android.hardware.camera.autofocus" /> <!--  自动聚焦权限  -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/w"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".my.activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".my.activity.myactivity.My_DataActivity" />
        <activity android:name=".my.activity.myactivity.My_CircleActivity" />
        <activity android:name=".my.activity.myactivity.My_FootprintsActivity" />
        <activity android:name=".my.activity.myactivity.My_WalletActivity" />
        <activity android:name=".my.activity.myactivity.My_AddressActivity" />
        <activity android:name=".my.activity.login_register.RegisterActivity" />
        <activity android:name=".my.activity.login_register.LoginActivity">

        </activity>
        <activity android:name=".my.activity.sou.SouActivity" />
        <activity android:name=".my.activity.sou.ShowActivity" />
        <activity android:name=".my.activity.sou.InfoActivity"></activity>
    </application>

</manifest>